@model DiscoverySetting

<h2>Create New Setting</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="SettingId"></label>
        <input type="text" class="form-control" asp-for="SettingId" required />
    </div>

    <div class="form-group">
        <label asp-for="Parameter"></label>
        <input type="text" class="form-control" asp-for="Parameter" required />
    </div>

    <div class="form-group">
        <label asp-for="Value"></label>
        <input type="text" class="form-control" asp-for="Value" required />
    </div>

    <button type="submit" class="btn btn-success">Create</button>
    <a class="btn btn-secondary" asp-action="Index">Cancel</a>
</form>
